// example 

var uid-number-bands = require('./index');

var BAND = 0;
var BANDS = 100;

uid-number-bands.init(BAND, BANDS, function() {

	var id = uid-number-bands.make();
	console.log( 'id: ' + id );
	
	var idNormalized = uid-number-bands.normalize(id);
	console.log( 'idNormalized: ' + idNormalized );
	
});
